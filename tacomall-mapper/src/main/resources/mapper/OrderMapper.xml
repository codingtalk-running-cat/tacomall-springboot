<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.codingtalk.tacomallmapper.order.OrderMapper">

    <resultMap id="getOrderPage" type="cn.codingtalk.tacomallentity.order.Order">
        <id property="id" column="id" />
    </resultMap>

    <select id="getOrderPage" resultMap="getOrderPage">
        SELECT *
        FROM order
        WHERE member_id=#{memberId}
    </select>


</mapper>